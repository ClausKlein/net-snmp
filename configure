      hardcode_libdir_separator_GCJ=:
      hardcode_shlibpath_var_GCJ=no
      ;;

    openbsd*)
      if test -f /usr/libexec/ld.so; then
	hardcode_direct_GCJ=yes
	hardcode_shlibpath_var_GCJ=no
